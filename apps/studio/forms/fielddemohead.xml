<?xml version="1.0" encoding="UTF-8"?>
<form name="fielddemohead" title="fielddemohead.label" primarykey="fielddemohead_id">
	<field name="upload_date" type="date">
		<element name="upload_date" type="text" label="upload_date.label" placeholder="upload_date.placeholder">
<class>datepicker</class>
</element>
	</field>
	<field name="district_id" type="int" length="11">
		<element name="district_id" type="select" label="district_id.label" placeholder="district_id.placeholder" lookup="lkpdistrict" display="district_des" value="district_id"></element>
	</field>
	<field name="village_id" type="int" length="11">
		<element name="village_id" type="select" label="village_id.label" placeholder="village_id.placeholder" lookup="lkpvillage" display="village_des" value="village_id"></element>
	</field>
	<field name="subvillage_name" type="int" length="120">
		<element name="subvillage_name" type="text" label="subvillage_name.label" placeholder="subvillage_name.placeholder"></element>
	</field>
	<field name="facilitator_name" type="int" length="120">
		<element name="facilitator_name" type="text" label="facilitator_name.label" placeholder="facilitator_name.placeholder"></element>
	</field>
	<field name="fielddemo_year" type="int" length="11">
		<element name="fielddemo_year" type="text" label="fielddemo_year.label" placeholder="fielddemo_year.placeholder"></element>
	</field>
	<field name="fielddemo_season" type="varchar" length="45">
		<element name="fielddemo_season" type="text" label="fielddemo_season.label" placeholder="fielddemo_season.placeholder"></element>
	</field>
	<field name="submit">
		<element name="action" type="submit" value="action.submit"></element>
	</field>
	<field name="reset">
		<element name="action" type="reset" value="action.reset"></element>
	</field>
</form>