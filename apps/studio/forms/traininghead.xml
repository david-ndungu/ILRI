<?xml version="1.0" encoding="UTF-8"?>
<form name="traininghead" title="traininghead.label" primarykey="traininghead_id">
	<field name="upload_date" type="date">
		<element name="upload_date" type="text" label="upload_date.label" placeholder="upload_date.placeholder">
<class>datepicker</class>
</element>
	</field>
	<field name="district_id" type="int" length="11">
		<element name="district_id" type="select" label="district_id.label" placeholder="district_id.placeholder" lookup="lkpdistrict" display="district_des" value="district_id"></element>
	</field>
	<field name="village_id" type="int" length="11">
		<element name="village_id" type="select" label="village_id.label" placeholder="village_id.placeholder" lookup="lkpvillage" display="village_des" value="village_id"></element>
	</field>
	<field name="training_topic" type="varchar" length="120">
		<element name="training_topic" type="text" label="training_topic.label" placeholder="training_topic.placeholder"></element>
	</field>
	<field name="ward_name" type="varchar" length="120">
		<element name="ward_name" type="text" label="ward_name.label" placeholder="ward_name.placeholder"></element>
	</field>
	<field name="subvillage_name" type="varchar" length="120">
		<element name="subvillage_name" type="text" label="subvillage_name.label" placeholder="subvillage_name.placeholder"></element>
	</field>
	<field name="facilitator_name" type="varchar" length="120">
		<element name="facilitator_name" type="text" label="facilitator_name.label" placeholder="facilitator_name.placeholder"></element>
	</field>
	<field name="participant_name" type="varchar" length="120">
		<element name="participant_name" type="text" label="participant_name.label" placeholder="participant_name.placeholder"></element>
	</field>
	<field name="participant_group" type="varchar" length="120">
		<element name="participant_group" type="text" label="participant_group.label" placeholder="participant_group.placeholder"></element>
	</field>
	<field name="training_date" type="date">
		<element name="training_date" type="text" label="training_date.label" placeholder="training_date.placeholder">
<class>datepicker</class>
</element>
	</field>
	<field name="participant_gender" type="int" length="11">
		<element name="participant_gender" type="text" label="participant_gender.label" placeholder="participant_gender.placeholder"></element>
	</field>
	<field name="submit">
		<element name="action" type="submit" value="action.submit"></element>
	</field>
	<field name="reset">
		<element name="action" type="reset" value="action.reset"></element>
	</field>
</form>